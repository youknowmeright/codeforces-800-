/*

https://codeforces.com/problemset/problem/1742/A
*/


#include<stdio.h>
int main()
{

  int x;
  int a,b,c;
  scanf("%d",&x);
  while(x--)
  {

     scanf("%d%d%d",&a,&b,&c);

     if(a==b+c||c==b+a||b==a+c)
     {

      printf("YES\n");
     }
     else
     {
      printf("NO\n");
     }


  
  }
  return 0;


}
