/*
 https://codeforces.com/problemset/problem/1758/A
*/

#include<iostream>
#include<algorithm>
using namespace std;

int main()
{
    int t;
    int x;
    cin>>t;

    while(t--)
    {
        string s;
        cin>>s;
         x=s.length();

        cout<<s;

        for(int i=x-1;i>=0;i--)
        {
           cout<<s[i];


        }
           cout<<endl;



    }
       return 0;

}
