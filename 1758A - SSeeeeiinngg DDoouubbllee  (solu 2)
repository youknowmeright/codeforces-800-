/*

 https://codeforces.com/problemset/problem/1758/A
*/
#include<iostream>
#include<algorithm>
using namespace std;

int main()
{
 int t;
 cin>>t;

 while(t--)
 {
      string s;
      string temp;
      cin>>s;
           cout<<s;
      temp=s;

      reverse(temp.begin(),temp.end());       // method of reversing a string using c++ algorithm//


       



      
      cout<<temp<<endl;



 }
return 0;

}
