#include<iostream>
using namespace std;
int main(){
    int x;
    cin >> x;
    while(x--){
        int n;
        cin >> n;
        int a[n];
        for(int i = 1;i<=n;i++){
            cout << i*2 << " ";
        }
        cout<< endl;
    }
}

