/*
https://codeforces.com/problemset/problem/1760/A
*/



#include<stdio.h>
int main()
{
    
    int a[500];
    int temp;
    int i, j,x; 
    scanf("%d",&x);


    while (x--)
    {
     
    
    for(i=0; i<3; i++)
    {
        scanf("%d ",&a[i]);
    }






    
    for(i=0; i<3; i++)
    {
        for(j=i+1; j<3; j++) 
        {      if(a[i]>a[j])
            {
                temp = a[i];
                a[i] = a[j];
                a[j] = temp;
            }
        }
    }
   
    for(i=1; i<=1; i++)
    {
        printf("%d\n",a[1]);
    }
    }
    return 0;
}
