/*
https://codeforces.com/problemset/problem/1676/A
*/

#include <iostream>
using namespace std;
int main()
{
    int x;
    
    cin>>x;
    int arr[20];
    while(x--)
    {
        string arr;
        cin>>arr;
int ac=0;int bc=0;
        for(int i=0;i<=2;i++)
        {
         ac=ac+arr[i];
        }
        
        for(int i=3;i<6;i++)
        {
         bc=bc+arr[i];
         
         
        }
       
        if(ac==bc)
        {
            cout<<"YES"<<endl;
        }
        else{
            cout<<"NO"<<endl;
        }

    }
    return 0;

}
